  "dependencies": {
    "axios": "^0.18.0",
    "concurrently": "^5.3.0",
    "cors": "^2.8.5",
    "express": "^4.16.3",
    "express-graphql": "^0.6.12",
    "graphql": "^0.13.2",
    "nodemon": "^1.18.1"
  }
